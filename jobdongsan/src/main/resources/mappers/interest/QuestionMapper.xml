<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.jobdongsan.project.dao.IQuestionDAO">
	
	<!-- 질문 가져오기 -->
	<select id="getQuestionCon" parameterType="String" resultType="String">
		select questionCon from question 
	</select>
	
	<!-- 질문 캐릭터 가져오기 -->
	<select id="getQuestionChar" parameterType="String" resultType="String">
		select questionChar from question 
	</select>
</mapper>
